    "related_applications": [
        {
            "platform": "webapp",
            "url": "https://kingueletrical.com/manifest.json"
        }
    ],
    
    "prefer_related_applications": false,
    
    "protocol_handlers": [
        {
            "protocol": "web+kingushop",
            "url": "/Kinguelectrical-shop.html?product=%s"
        },
        {
            "protocol": "web+kingu",
            "url": "/?service=%s"
        },
        {
            "protocol": "tel",
            "url": "tel:%s"
        }
    ],
    
    "share_target": {
        "action": "/Kinguelectrical-shop.html",
        "method": "GET",
        "enctype": "application/x-www-form-urlencoded",
        "params": {
            "title": "name",
            "text": "description",
            "url": "link"
        }
    },
    
    "handle_links": "preferred",
    
    "edge_side_panel": {
        "preferred_width": 400
    },
    
    "launch_handler": {
        "client_mode": "auto"
    },
    
    "display_override": ["window-controls-overlay", "standalone"],
    
    "permissions": [
        "notifications",
        "storage",
        "background-sync",
        "periodic-background-sync",
        "clipboard-read",
        "clipboard-write"
    ],
    
    "serviceworker": {
        "src": "/service-worker.js",
        "scope": "/",
        "update_via_cache": "none"
    },
    
    "shortcuts_icons": {
        "96": "/assets/icons/icon-96x96.png"
    },
    
    "file_handlers": [
        {
            "action": "/Kinguelectrical-shop.html",
            "accept": {
                "text/*": [".txt", ".text"],
                "application/pdf": [".pdf"]
            }
        }
    ],
    
    "description": "Kingu Electrical - Professional electrical engineering services and products in Tanzania. Generator installation, solar systems, PLC programming, HPU conversion, and electrical equipment supply.",
    
    "screenshots_icons": {
        "form_factors": ["wide", "narrow"],
        "label": "Kingu Electrical App Screenshots"
    },
    
