            "name": "WhatsApp"
        }
    ],
    
    "prefer_related_applications": true,
    
    "protocol_handlers": [
        {
            "protocol": "web+kingushop",
            "url": "/Kinguelectrical-shop.html?product=%s"
        },
        {
            "protocol": "web+kingu",
            "url": "/?service=%s"
        }
    ],
    
    "share_target": {
        "action": "/Kinguelectrical-shop.html",
        "method": "GET",
        "enctype": "application/x-www-form-urlencoded",
        "params": {
            "title": "name",
            "text": "description",
            "url": "link"
        }
    },
    
    "handle_links": "preferred",
    
    "edge_side_panel": {
        "preferred_width": 360
    },
    
    "permissions": [
        "notifications",
        "storage",
        "background-sync",
        "periodic-background-sync"
    ],
    
    "serviceworker": {
        "src": "/service-worker.js",
        "scope": "/",
        "update_via_cache": "none"
    }
