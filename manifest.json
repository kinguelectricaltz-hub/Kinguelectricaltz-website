        "params": {
            "title": "name",
            "text": "description",
            "url": "link",
            "files": [
                {
                    "name": "image",
                    "accept": ["image/*"]
                }
            ]
        }
    },
    
    "file_handlers": [
        {
            "action": "/tools/quote-generator",
            "accept": {
                "application/pdf": [".pdf"],
                "image/*": [".jpg", ".jpeg", ".png", ".webp"],
                "text/plain": [".txt"]
            },
            "icons": [{
                "src": "/assets/icons/file-handler.png",
                "sizes": "64x64",
                "type": "image/png"
            }]
        }
    ],
    
    "protocol_handlers": [
        {
            "protocol": "web+kingu",
            "url": "/?service=%s"
        },
        {
            "protocol": "web+kingushop",
            "url": "/Kinguelectrical-shop.html?product=%s"
        },
        {
            "protocol": "mailto",
            "url": "/contact?email=%s"
        }
    ],
    
    "launch_handler": {
